record(stringout, "$(P)$(R)DEBUG"){
    field(DTYP, "stream")
    field(OUT,  "@devU2021XA.proto debug $(PORT) $(A)")
}

record(waveform, "$(P)$(R)TracData-Mon"){
    field(SCAN, "5 second")
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto getTracData $(PORT) $(A)")
    field(FTVL, "FLOAT")
    field(NELM, "26000")
}

record(stringin, "$(P)$(R)Resources-Mon"){
    field(SCAN, "10 second")
    field(DESC, "Available Visa resources")
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto getResources $(PORT) $(A)")
}

record(stringin, "$(P)$(R)InstrInfo-Mon"){
    field(SCAN, "10 second")
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto getInstrInfo $(PORT) $(A)")
}

record(ai, "$(P)$(R)TracTime:RB"){
    field(SCAN, "10 second")
    field(DESC, "TRAC:TIME for the trac data record")
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto getTracTime $(PORT) $(A)")
}

record(ao, "$(P)$(R)TracTime:SP"){
    field(DESC, "TRAC:TIME for the trac data record")
    field(DTYP, "stream")
    field(OUT,  "@devU2021XA.proto setTracTime $(PORT) $(A)")

    field(FLNK, "$(P)$(R)TracTime:RB")
}

record(stringin, "$(P)$(R)IDN-Mon"){
    field(SCAN, "10 second")
    field(DESC, "SCPI identification string")
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto query(*IDN?) $(PORT) $(A)")
}

record(stringout, "$(P)$(R)InstrDisconnect-Cmd"){
    field(DTYP, "stream")
    field(OUT,  "@devU2021XA.proto instrDisconnect $(PORT) $(A)")
}

record(stringout, "$(P)$(R)InstrConnect-Cmd"){
    field(DTYP, "stream")
    field(OUT,  "@devU2021XA.proto instrConnect $(PORT) $(A)")
}

record(stringout, "$(P)$(R)InstrInit-Cmd"){
    field(DTYP, "stream")
