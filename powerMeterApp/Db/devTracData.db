record(stringout, "$(P)$(R)TracData_egu") {
    field(OMSL, "closed_loop")
    field(DOL,  "$(P)$(R)Egu-RB CP")
    field(OUT,  "$(P)$(R)TracData-Mon.EGU")
}
record(stringout, "$(P)$(R)TracDataAvg_egu") {
    field(OMSL, "closed_loop")
    field(DOL,  "$(P)$(R)Egu-RB CP")
    field(OUT,  "$(P)$(R)TracDataAvg-Mon.EGU")
}
record(waveform, "$(P)$(R)TracData-Mon") {
    field(PREC, "4")
    field(DESC, "Trace data")
    field(DTYP, "stream")
    field(SCAN, "I/O Intr")
    field(INP,  "@devU2021XA.proto getTracData $(FROM) $(A)")
    field(FTVL, "FLOAT")
    field(NELM, "240")
}
record(waveform, "$(P)$(R)TimeAxis-Mon") {
    field(DESC, "Trace data time axis")
    field(EGU,  "s")
    field(SCAN, "I/O Intr")
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto getTimeAxis $(FROM) $(A)")
    field(FTVL, "FLOAT")
    field(NELM, "240")
}
record(compress, "$(P)$(R)TracDataAvg-Mon") {
    field(DESC, "Trace data average")
    field(INP,  "$(P)$(R)TracData-Mon CP MSS")
    field(ALG,  "N to 1 Average")
    field(N,    "240")
    field(NSAM, "1")
    field(PREC, "4")
}
record(ao, "$(P)$(R)TracTime-SP") {
    field(DESC, "TRAC:TIME used to acquire data")
    field(DTYP, "stream")
    field(PREC, "4")
    field(OUT,  "@devU2021XA.proto setTracTime $(TO) $(A)")
    field(FLNK, "$(P)$(R)TracTime-RB")
}
record(bi, "$(P)$(R)TracTime-RB_start") {
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto getTracTime_start $(TO) $(A)")
    field(PHAS, "0")
    field(PINI, "YES")
    field(SCAN, "Passive")
}
record(ai, "$(P)$(R)TracTime-RB") {
    field(EGU,  "s")
    field(DESC, "Duration of the trace")
    field(SCAN, "I/O Intr")
    field(DTYP, "stream")
    field(PREC, "4")
    field(INP,  "@devU2021XA.proto getTracTime_end $(FROM) $(A)")
}
