record(mbbo, "$(P)$(R)Egu-SP"){
    field(DESC, "Pressure Unit")
    field(DTYP, "stream")
    field(OUT,  "@devU2021XA.proto write_str(50,:TRAC:UNIT) $(PORT) $(A)")

    field(UNSV, "INVALID")
    field(IVOA, "Don't drive outputs")

    field(ZRVL, "0")
    field(ONVL, "1")
    field(ZRST, "W")
    field(ONST, "dBm")
    
    field(DISV, "1")
    field(SDIS, "$(P)$(R)Egu_init.PACT")

    field(FLNK, "$(P)$(R)Egu-RB")
}
record(stringin, "$(P)$(R)Egu-RB"){
    field(PINI, "YES")
    field(SCAN, "Passive")
    field(DTYP, "stream")
    field(INP,  "@devU2021XA.proto query(50,:TRAC:UNIT?) $(PORT) $(A)")
    field(PHAS, "0")
}
record(seq, "$(P)$(R)Egu_init"){
    field(PINI, "YES")
    field(PHAS, "1")
    field(DLY1, "1")
    field(DOL1, "$(P)$(R)Egu-RB")
    field(LNK1, "$(P)$(R)Egu-SP PP")
}
