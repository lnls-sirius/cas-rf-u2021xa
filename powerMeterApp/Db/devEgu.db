record(mbbo, "$(P)$(R)Egu-SP"){
    field(DESC, "Pressure Unit")
    field(DTYP, "stream")
    field(OUT,  "@devTest.proto write_str(:TRAC:UNIT) $(TO) $(A)")

    field(UNSV, "INVALID")
    field(IVOA, "Don't drive outputs")

    field(ZRVL, "0")
    field(ONVL, "1")
    field(ZRST, "W")
    field(ONST, "DBM")

    field(FLNK, "$(P)$(R)Egu-RB_start")
}
record(bi, "$(P)$(R)Egu-RB_start"){
    field(DTYP, "stream")
    field(INP,  "@devTest.proto query_start(:TRAC:UNIT?) $(TO) $(A)")
    field(PHAS, "0")
    field(PINI, "YES")
    field(SCAN, "Passive")
}
record(stringin, "$(P)$(R)Egu-RB"){
    field(SCAN, "I/O Intr")
    field(DTYP, "stream")
    field(INP,  "@devTest.proto query_end(:TRAC:UNIT?) $(FROM) $(A)")
}