record(bo, "$(P)$(R)CalAll-Cmd") {
    field(DESC, "Zero and calibration")
    field(DTYP, "stream")
    field(OUT,  "@devTest.proto cmd(:CAL:ALL) $(TO) $(A)")
}

record(bi, "$(P)$(R)IDN-Mon_start") {
    field(SCAN, "10 second")
    field(DTYP, "stream")
    field(INP,  "@devTest.proto query_start(*IDN?) $(TO) $(A)")
}
