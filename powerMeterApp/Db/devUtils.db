    field(NELM, "200")
    field(FTVL, "UCHAR")
    field(SCAN, "I/O Intr")
    field(DESC, "SCPI identification string")
    field(DTYP, "stream")
    field(INP,  "@devTest.proto query_end_ml(*IDN?,199) $(FROM) $(A)")
}

record(bi, "$(P)$(R)SensMode-Mon_start") {
    field(SCAN, "2 second")
